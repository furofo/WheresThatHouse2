<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <TextView
        android:id = "@+id/texty"
        android:layout_width="368dp"

        android:layout_height="100dp"
        android:gravity="center"
        android:text="Click Neighboorhood"
        android:textSize="30sp"

        android:layout_marginTop="37dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


    <Button
        android:id="@+id/WAL_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="WAL"

        android:layout_alignBaseline="@+id/SSB_button"
        android:layout_alignBottom="@+id/SSB_button"
        android:layout_alignLeft="@+id/MMB_button"
        android:layout_alignStart="@+id/MMB_button" />

    <Button
        android:id="@+id/GBR_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="GBR"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/BLP_button"
        android:layout_alignStart="@+id/BLP_button" />

    <Button
        android:id="@+id/WGE_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="WGE"
        android:layout_below="@+id/RVL_button"
        android:layout_alignLeft="@+id/RVL_button"
        android:layout_alignStart="@+id/RVL_button"
        android:layout_marginTop="22dp" />

    <Button
        android:id="@+id/MMB_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="MMB"
        android:layout_alignBaseline="@+id/LHC_button"
        android:layout_alignBottom="@+id/LHC_button"
        android:layout_alignLeft="@+id/GBR_button"
        android:layout_alignStart="@+id/GBR_button" />

    <Button
        android:id="@+id/ADL_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="ADL"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="21dp"
        android:layout_above="@+id/CPR_button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/SSB_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="SSB"
        android:layout_alignBaseline="@+id/RVL_button"
        android:layout_alignBottom="@+id/RVL_button"
        android:layout_alignLeft="@+id/LHC_button"
        android:layout_alignStart="@+id/LHC_button" />
    <Button
        android:id="@+id/BLP_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="BLP"
        android:layout_alignBaseline="@+id/ARB_button"
        android:layout_alignBottom="@+id/ARB_button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp" />

    <Button
        android:id="@+id/LHC_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="LHC"
        android:layout_alignBaseline="@+id/HOR_button"
        android:layout_alignBottom="@+id/HOR_button"
        android:layout_alignLeft="@+id/FHH_button"
        android:layout_alignStart="@+id/FHH_button" />

    <Button
        android:id="@+id/RVL_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="RVL"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/HOR_button"
        android:layout_alignLeft="@+id/HOR_button"
        android:layout_alignStart="@+id/HOR_button" />
    <Button
        android:id="@+id/HOR_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="HOR"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/CPR_button"
        android:layout_alignLeft="@+id/CPR_button"
        android:layout_alignStart="@+id/CPR_button" />

    <Button
        android:id="@+id/ARB_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="ARB"
        android:layout_alignBaseline="@+id/ADL_button"
        android:layout_alignBottom="@+id/ADL_button"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/FHH_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="FHH"
        android:layout_alignTop="@+id/GBR_button"
        android:layout_alignLeft="@+id/ARB_button"
        android:layout_alignStart="@+id/ARB_button" />

    <Button
        android:id="@+id/CPR_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="CPR"
        android:layout_alignBaseline="@+id/FHH_button"
        android:layout_alignBottom="@+id/FHH_button"
        android:layout_alignLeft="@+id/ADL_button"
        android:layout_alignStart="@+id/ADL_button" />




</RelativeLayout>
